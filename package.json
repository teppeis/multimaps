{
  "name": "@teppeis/multimaps",
  "description": "Multimap classes for TypeScript and JavaScript",
  "version": "2.0.0",
  "author": "Teppei Sato <teppeis@gmail.com>",
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=14"
  },
  "type": "module",
  "main": "./dist/cjs/src/index.js",
  "types": "./dist/cjs/src/index.d.ts",
  "module": "./dist/esm/src/index.js",
  "exports": {
    ".": {
      "import": "./dist/esm/src/index.js",
      "require": "./dist/cjs/src/index.js",
      "default": "./dist/cjs/src/index.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist/cjs/src",
    "dist/esm/src"
  ],
  "scripts": {
    "build": "run-p build:*",
    "build:cjs": "tsc --project tsconfig.cjs.json",
    "build:esm": "tsc",
    "clean": "rimraf 'dist/!(cjs)' 'dist/cjs/!(package.json)'",
    "lint:eslint": "eslint .",
    "lint:prettier": "prettier --check .",
    "fix": "run-s fix:prettier fix:eslint",
    "fix:eslint": "npm run lint:eslint -- --fix",
    "fix:prettier": "npm run lint:prettier -- --write",
    "prepublishOnly": "run-s clean build",
    "test": "run-p -c --aggregate-output lint:* type unit",
    "type": "tsc --noEmit",
    "unit": "vitest run --coverage"
  },
  "devDependencies": {
    "@types/node": "^14.18.16",
    "c8": "^7.11.2",
    "eslint": "^8.14.0",
    "eslint-config-teppeis": "^15.0.1",
    "npm-run-all": "^4.1.5",
    "power-assert": "^1.6.1",
    "prettier": "^2.6.2",
    "rimraf": "^3.0.2",
    "typescript": "^4.7.0-beta",
    "vitest": "^0.9.4"
  },
  "homepage": "https://github.com/teppeis/multimaps#readme",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/teppeis/multimaps.git"
  },
  "bugs": {
    "url": "https://github.com/teppeis/multimaps/issues"
  },
  "keywords": [
    "map",
    "multi-map",
    "multimap",
    "typescript"
  ],
  "license": "MIT"
}
